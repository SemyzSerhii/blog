<div class= "center">
  <h1 class="Sign_UP"> Log In</h1>
  <%= form_tag sessions_path do %>
    <div class="form-group">
      <div class="col-md-6">
        <%= label_tag :username , params[:username], class:"User_save" %><br />
        <%= text_field_tag :username , params[:username], class: "form-control" , placeholder: "Enter Your Username"%>
      </div>
    </div>
    <div class="form-group">
      <div class="col-md-6">
        <%= label_tag :password, params[:password], class: "but_Password_save" %><br />
        <%= password_field_tag :password, params[:password],class: "form-control",  placeholder: "Enter Your Password"  %>
      </div>
    </div>
    <div class="but_Data_saveX2 "><button type="submit" class="btn btn-primary col-md-3">Login</button></div>
  <% end %>
</div>
